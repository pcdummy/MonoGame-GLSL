#monogame EffectParameter(name=OriginalIntensity; class=Scalar; type=Single; semantic=; rows=1; columns=1; elements=[]; structMembers=[])
#monogame EffectParameter(name=BloomSaturation; class=Scalar; type=Single; semantic=; rows=1; columns=1; elements=[]; structMembers=[])
